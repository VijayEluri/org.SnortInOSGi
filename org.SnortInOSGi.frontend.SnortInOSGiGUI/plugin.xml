<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.snortinosgi.frontend.snortinosgigui.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="Perspective"
            class="org.snortinosgi.frontend.snortinosgigui.Perspective"
            id="org.SnortInOSGi.frontend.SnortInOSGiGUI.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            name="View"
            class="org.snortinosgi.frontend.snortinosgigui.views.View"
            id="org.SnortInOSGi.frontend.SnortInOSGiGUI.view">
      </view>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.SnortInOSGi.frontend.SnortInOSGiGUI.application"
            name="RCP Application">
         <property
               name="windowImages"
               value="icons/alt_window_16.gif,icons/alt_window_32.gif">
         </property>
      </product>
   </extension>
   <extension
         id="product1"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.SnortInOSGi.frontend.SnortInOSGiGUI.application"
            name="org.SnortInOSGi.frontend.SnortInOSGiGUI.product">
         <property
               name="appName"
               value="org.SnortInOSGi.frontend.SnortInOSGiGUI.product">
         </property>
      </product>
   </extension>
   <extension
         id="product2"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.SnortInOSGi.frontend.SnortInOSGiGUI.application"
            name="org.SnortInOSGi.frontend.SnortInOSGiGUI.product">
         <property
               name="appName"
               value="org.SnortInOSGi.frontend.SnortInOSGiGUI.product">
         </property>
      </product>
   </extension>

</plugin>
